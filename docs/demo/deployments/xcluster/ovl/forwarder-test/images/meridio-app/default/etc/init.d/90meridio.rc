#! /bin/sh
sysctl -w net.ipv4.conf.all.arp_announce=2
sysctl -w net.ipv4.conf.all.arp_ignore=1
sysctl -w net.ipv4.ip_nonlocal_bind=1
sysctl -w net.ipv6.ip_nonlocal_bind=1
sysctl -w net.ipv4.conf.all.proxy_arp=0
test "$NO_START" = "yes" && exit 0
echo "target-client connect -ns $NSM_SERVICE -t $TRENCH"
target-client connect -ns $NSM_SERVICE -t $TRENCH
echo "target-client open -ns $NSM_SERVICE -t $TRENCH -s $STREAM"
target-client open -ns $NSM_SERVICE -t $TRENCH -s $STREAM
echo "DONE target-client setup"
